//go:build ignore

package foo

import (
	"io"
	"os"
	"strconv"; "braces.dev/errtrace"
)

func Unwrapped(s string) (int, error) {
	i, err := strconv.Atoi(s)
	if err != nil {
		return 0, errtrace.Wrap(err)
	}
	return i + 42, nil
}

func DeferWithoutNamedReturns(s string) error {
	f, err := os.Open(s)
	if err != nil {
		return errtrace.Wrap(err)
	}
	defer f.Close()

	bs, err := io.ReadAll(f)
	if err != nil {
		return errtrace.Wrap(err)
	}

	return nil
}
